algoritmo "somaDoisNumeros"
var
   numero1, numero2, soma: inteiro
inicio

   escreva("Digite o numero 1: ")
   leia(numero1)
   escreva("Digite o numero 2: ")
   leia(numero2)
   soma <- numero1 + numero2
   escreva("A soma entre ", numero1, " e ", numero2, " tem como resultado: ",soma)

fimalgoritmo